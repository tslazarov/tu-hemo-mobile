<Page navigatingTo="onNavigatingTo" class="page" xmlns="http://schemas.nativescript.org/tns.xsd">
    <GridLayout rows="*,auto">
        <StackLayout orientation="vertical" verticalAlignment="center" row="0" class="p-t-15" id="RegisterPanel" backgroundColor="yellow">
            <TextField class="m-t-10 m-b-10 m-l-15 m-r-15" hint="{{ emailHint }}" keyboardType="email" autocorrect="false" autocapitalization="none"
                text="{{ email }}"></TextField>
            <TextField class="m-t-10 m-b-10 m-l-15 m-r-15" hint="{{ passwordHint }}" secure="true" autocorrect="false" autocapitalization="none"
                text="{{ password }}"></TextField>
            <TextField class="m-t-10 m-b-10 m-l-15 m-r-15" hint="{{ confirmPasswordHint }}" secure="true" autocorrect="false" autocapitalization="none"
                text="{{ confirmPassword }}">
            </TextField>
            <Button class="btn btn-primary" text="{{ next }}" tap="onNextTap"></Button>
        </StackLayout>
        <StackLayout row="1" backgroundColor="red">
            <Label text="asd"/>
        </StackLayout>
    </GridLayout>
</Page>